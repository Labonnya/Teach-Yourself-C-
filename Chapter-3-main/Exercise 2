#include <stdio.h>
int main ()
{
    float d,v;
    int i,x;
    printf("Enter number of computations:\n");
    scanf("%d",&i);
    for(x=1;x<=i;x++)
    {
        printf("Enter distance in meters and speed in meters per second: \n");
        scanf("%f %f",&d,&v);
        printf("driving time is: %f seconds\n",d/v);
    }
    return 0;
}
