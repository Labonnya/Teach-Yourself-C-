#include <stdio.h>
int main ()
{
    int i;
    char c;
    for(i=1; i<100; i++)
    {
        if(!(i%6))
        {
            printf("%d, more?(Y/N)",i);
            c=getchar();
            getchar();
            if(c=='N')
                break;
            printf("\n");
        }
    }
    return 0;
}
