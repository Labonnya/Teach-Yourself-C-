#include <stdio.h>
int main ()
{
    int num[10],i;
    for(i=0;i<10;i++)
    {
        scanf("%d",&num[i]);
    }
    for(i=0;i<9;i++)
    {
        if(num[i]=num[i+1])
        {printf("YES"); return 0;} }
    for(i=0;i<8;i++)
    {
        if(num[i]=num[i+2])
        {printf("YES"); return 0;}}
    for(i=0;i<7;i++)
    {
        if(num[i]=num[i+3])
        {printf("YES"); return 0;}}
    for(i=0;i<6;i++)
    {
        if(num[i]=num[i+4])
        {printf("YES"); return 0;}}
    for(i=0;i<5;i++)
    {
        if(num[i]=num[i+5])
        {printf("YES"); return 0;}}
    for(i=0;i<4;i++)
    {
        if(num[i]=num[i+6])
        {printf("YES"); return 0;}}
    for(i=0;i<3;i++)
    {
        if(num[i]=num[i+7])
        {printf("YES"); return 0;}}
    for(i=0;i<2;i++)
    {
        if(num[i]=num[i+8])
        {printf("YES"); return 0;}}
    for(i=0;i<1;i++)
    {
        if(num[i]=num[i+9])
        {printf("YES"); return 0;}}

    printf("No");
    return 0;


}
