#include <stdio.h>
int main ()
{
    int c[][3]={1,1,1,2,4,8,3,9,27,4,16,64,5,25,125,6,36,216,7,49,343,8,64,512,9,81,729,10,100,1000},i,j;
    printf("Enter cube:");
    scanf("%d",&j);

    for(i=0;i<10;i++)
        {
           if(c[i][2]==j)
           {
               printf("Root is %d\n",c[i][0]);
               printf("Square of root is %d",c[i][1]);
           }
           if(i==10) printf("cube not found");
        }

}
